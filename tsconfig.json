{
  "compilerOptions": {
    "types": [
      "next"
    ],
    "paths": {
      "@payload-config": [
        "payload.config.ts"
      ]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "module": "node16",
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "."
    ,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/(payload)/_collection/Comments.js", "src/app/(payload)/_collection/orders.js", "src/app/(payload)/_collection/Products.js", "src/app/(payload)/lib/payload.js", "src/app/(payload)/api/graphql/route.js", "src/app/(payload)/api/graphql-playground/route.js", "src/app/(payload)/_collection/User.js", "src/app/(payload)/_collection/User.js"  ],
  "exclude": [
    "node_modules"
  ]
}
